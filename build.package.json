{
  "name": "mist-kit",
  "version": "0.0.3",
  "peerDependencies": {
    "react": "18.2.0",
    "tailwindcss": "4.0.0-alpha.13"
  },
  "main": "./dist/esm/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/esm/index.js"
    }
  },
  "style": "./dist/esm/main.css",
  "typings": "dist/types/index.d.ts",
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build-storybook": "storybook build",
    "build:mist": "mistcss --watch ./src",
    "build:docs": "next build",
    "build": "tsc --project tsconfig.build.json && cp ./src/main.css ./dist/esm/main.css",
    "lint": "eslint ./src/primals ./src/atoms ./app --fix",
    "prepare": "husky"
  }
}
